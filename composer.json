{
    "name": "bencavens/omnipay-globalcollect",
    "type": "library",
    "description": "Globalcollect driver for the Omnipay payment processing library",
    "keywords": [
        "gateway",
        "merchant",
        "globalcollect",
        "omnipay",
        "pay",
        "payment"
    ],
    "homepage": "https://github.com/bencavens/omnipay-globalcollect",
    "license": "MIT",
    "authors": [
        {
            "name": "Ben Cavens",
            "email": "cavensben@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": { "Omnipay\\Globalcollect\\" : "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Omnipay\\Globalcollect\\Tests\\" : "tests/" }
    },
    "require": {
        "omnipay/common": "^2.5",
        "omnipay/tests": "^2.1"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^1.5.6"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 src/",
        "fix-style": "phpcbf -p --standard=PSR2 src/"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
